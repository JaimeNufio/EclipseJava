
public class phieffect {

	public static void main(String args[]) throws InterruptedException {
		String text = "";
		int i = 0, a = 0;
		while (true){
			text = "";
			for (i = 0; i <= 10; i++){
				
				for(a = 1; a < i;i++){
					text = text+"-";
				}
				text = text+"Hey";
				//for(int b = i-10 ){
					
				System.out.println(text);
			}
		}
	}
}
